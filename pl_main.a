;============================
pl_init:
        ;FIRST: Clear all ZP variables
        ldy #PL_ZP_END-PL_ZP_START
        lda #0
-       sta PL_ZP_START-1,y
        dey
        bne -

        ;THEN: Ensure that the first step is executed at once 
		lda #1
		sta PL_ZP_TICKCOUNTER
                
        rts
